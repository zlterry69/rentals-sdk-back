{
  "name": "rentals-back-infra",
  "version": "1.0.0",
  "description": "Infrastructure as Code for RENTALS-BACK using AWS CDK",
  "main": "bin/app.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy:dev": "cdk deploy --profile default --context stage=dev",
    "deploy:cert": "cdk deploy --profile default --context stage=cert",
    "deploy:prod": "cdk deploy --profile default --context stage=prod",
    "destroy": "cdk destroy",
    "synth": "cdk synth"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "20.8.10",
    "aws-cdk": "2.99.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.99.1",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  }
}
